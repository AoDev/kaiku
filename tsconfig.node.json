{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.rsbuild.config.*",
    "src/main/**/*",
    "src/preload/**/*",
    "electron.rsbuild.config.ts",
    "rsbuild-main.config.ts",
    "src/types/*.ts",
    "tools/**/*.ts",
    "src/lib/**/*.ts"
  ],
  "compilerOptions": {
    "composite": true
  }
}
