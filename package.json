{
  "name": "kaiku",
  "version": "1.0.0",
  "description": "Music Player for Local Files",
  "main": "./out/main/index.js",
  "private": true,
  "type": "commonjs",
  "author": "aodev",
  "homepage": "https://github.com/AoDev/kaiku",
  "scripts": {
    "build": "electron-rsbuild build",
    "build:linux": "electron-rsbuild build && electron-builder --linux",
    "build:mac": "electron-rsbuild build && electron-builder --mac --config electron-builder.config.js",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "builder": "electron-builder --dir",
    "check": "biome check --write",
    "dev": "electron-rsbuild dev",
    "format": "biome format --write",
    "icon-gen-osx": "icon-gen -i ./resources/icon-osx-app.png -o ./resources --ico --ico-name icon --icns --icns-name icon -r",
    "inspect": "bun --inspect ./node_modules/electron-rsbuild/bin/electron-rsbuild.mjs dev",
    "lint": "biome check src --diagnostic-level error && tsc --build --noEmit",
    "preview": "electron-rsbuild preview",
    "rsbuild:build": "rsbuild build",
    "rsbuild:build-main": "rsbuild build -c electron.rsbuild.config.ts",
    "rsbuild:build-preload": "rsbuild build rsbuild-preload.config.ts",
    "rsbuild:dev": "rsbuild dev",
    "start": "electron-rsbuild preview",
    "test": "vitest --config vitest.config.ts --run"
  },
  "dependencies": {
    "@electron-toolkit/preload": "3.0.1",
    "@electron-toolkit/utils": "3.0.0",
    "@rsbuild/core": "1.2.19",
    "@rsbuild/plugin-node-polyfill": "1.3.0",
    "@rsbuild/plugin-react": "1.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@electron-rsbuild/core": "0.0.10",
    "@electron-rsbuild/plugin-main": "0.0.10",
    "@electron-rsbuild/plugin-preload": "0.0.10",
    "@electron-rsbuild/plugin-renderer": "0.0.10",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@floating-ui/react": "0.27.5",
    "@rsbuild/plugin-less": "1.1.1",
    "@types/howler": "2.2.12",
    "@types/lodash": "4.17.16",
    "@types/node": "22.13.11",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "electron": "35.0.3",
    "electron-builder": "24.13.3",
    "fast-glob": "3.3.3",
    "howler": "2.2.4",
    "icon-gen": "5.0.0",
    "localforage": "1.10.0",
    "lodash": "4.17.21",
    "mobx": "6.13.7",
    "mobx-react": "9.2.0",
    "music-metadata": "11.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rsbuild-plugin-svg-icons": "0.1.1",
    "typescript": "5.8.2",
    "vitest": "3.0.9",
    "wavesurfer.js": "7.9.4"
  }
}
