{
  "name": "rs-electron",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "private": true,
  "type": "commonjs",
  "author": "veaba",
  "homepage": "https://electron-rsbuild.org",
  "scripts": {
    "build": "electron-rsbuild build",
    "build:linux": "electron-rsbuild build && electron-builder --linux",
    "build:mac": "electron-rsbuild build && electron-builder --mac",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "builder": "electron-builder --dir",
    "check": "biome check --write",
    "dev": "electron-rsbuild dev",
    "format": "biome format --write",
    "inspect": "bun --inspect ./node_modules/electron-rsbuild/bin/electron-rsbuild.mjs dev",
    "preview": "electron-rsbuild preview",
    "rsbuild:build": "rsbuild build",
    "rsbuild:build-main": "rsbuild build -c electron.rsbuild.config.ts",
    "rsbuild:build-preload": "rsbuild build rsbuild-preload.config.ts",
    "rsbuild:dev": "rsbuild dev",
    "start": "electron-rsbuild preview",
    "test": "vitest dev"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@rsbuild/core": "^1.1.0",
    "@rsbuild/plugin-node-polyfill": "^1.2.0",
    "@rsbuild/plugin-react": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@electron-rsbuild/core": "^0.0.10",
    "@electron-rsbuild/plugin-main": "^0.0.10",
    "@electron-rsbuild/plugin-preload": "^0.0.10",
    "@electron-rsbuild/plugin-renderer": "^0.0.10",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rsbuild/plugin-less": "1.1.1",
    "@types/howler": "2.2.12",
    "@types/lodash": "4.17.16",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "fast-glob": "3.3.3",
    "howler": "2.2.4",
    "lodash": "4.17.21",
    "mobx": "6.13.6",
    "mobx-react": "9.2.0",
    "music-metadata": "11.0.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rsbuild-plugin-svg-icons": "0.1.1",
    "typescript": "^5.5.2",
    "vitest": "^2.1.4"
  }
}
